<p id="notice"><%= notice %></p>

<h1>Membros</h1>

<table>
  <thead>
    <tr>
      <th>Id membro</th>
      <th>Nome</th>
      <th>Data aniversario</th>
      <th>Tipo</th>
      <th>Endereco</th>
      <th>Cep</th>
      <th>Telefone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membros.each do |membro| %>
      <tr>
        <td><%= membro.id_membro %></td>
        <td><%= membro.nome %></td>
        <td><%= membro.data_aniversario %></td>
        <td><%= membro.tipo %></td>
        <td><%= membro.endereco %></td>
        <td><%= membro.cep %></td>
        <td><%= membro.telefone %></td>
        <td><%= link_to 'Show', membro %></td>
        <td><%= link_to 'Edit', edit_membro_path(membro) %></td>
        <td><%= link_to 'Destroy', membro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membro', new_membro_path %>
