<%= form_for(celula) do |f| %>
  <% if celula.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(celula.errors.count, "error") %> prohibited this celula from being saved:</h2>

      <ul>
      <% celula.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nome %>
    <%= f.text_field :nome , class: "form-control"%>
  </div>

  <div class="field">
    <%= f.label :data_iniciada %>
    <%= f.text_field :data_iniciada , class: "form-control"%>
  </div>

  <div class="field">
    <%= f.label :endereco %>
    <%= f.text_field :endereco , class: "form-control"%>
  </div>

  <div class="field">
    <%= f.label :anfitriao_id %>
    <%= f.text_field :anfitriao_id , class: "form-control"%>
  </div>

  <div class="field">
    <%= f.label :membro_id %>
    <%= select("membro", "membro_id", Membro.all.collect {|p| [ p.nome, p.id ] }, { include_blank: true }, class: "form-control")%>
  </div>

  <div class="field">
    <%= f.label :dia %>
    <%= f.text_field :dia , class: "form-control"%>
  </div>

  <div class="field">
    <%= f.label :cep %>
    <%= f.text_field :cep , class: "form-control"%>
  </div>

  <div class="actions">
    <%= f.submit class: "btn btn-primary"%>
  </div>
<% end %>
